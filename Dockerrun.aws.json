{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "environment": [
        {
          "name": "POSTGRES_PASSWORD",
          "value": "secretpassword"
        },
        {
          "name": "POSTGRES_DB",
          "value": "medwebappdb"
        },
        {
          "name": "POSTGRES_PORT",
          "value": "5555"
        },
        {
          "name": "PGPORT",
          "value": "5555"
        }
      ],
      "essential": true,
      "image": "postgres",
      "name": "medwebpostgres",
      "portMappings": [
        {
          "containerPort": 5555,
          "hostPort": 5555
        }
      ],
      "memory": 600
    },
    {
      "essential": true,
      "image": "jodogne/orthanc",
      "name": "orthanc",
      "memory": 600
    },
    {
      "command": [
        "medwebuser:secretpassword:::upload"
      ],
      "essential": true,
      "image": "atmoz/sftp",
      "name": "sftp",
      "memory": 600
    }
  ],
  "family": "",
  "volumes": []
}
